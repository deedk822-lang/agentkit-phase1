# Quantum Observer Production Configuration
# Copy to .env and fill with your actual values

# Google Cloud Platform
GCP_PROJECT_ID=quantum-observer-prod
GCP_REGION=us-central1
GCP_ZONE=us-central1-a

# MCP Server Configuration
MCP_SERVER_URL=https://qo.deedk822.com/mcp/v1
MCP_PRIVATE_KEY=your-32-char-hex-key-here
CORS_ORIGINS=https://qo.deedk822.com,https://yourdomain.com

# AI API Keys
GROQ_API_KEY=gsk_your_groq_api_key_here
MISTRAL_API_KEY=your_mistral_api_key_here

# Google Docs Integration
GOOGLE_DOC_ID=your_command_queue_google_doc_id
GOOGLE_SERVICE_ACCOUNT_JSON=/app/config/service-account.json

# Notion Databases
NOTION_TOKEN=secret_your_notion_integration_token
NOTION_FEATURE_FLAGS_DB=your_feature_flags_database_id
NOTION_POLICIES_DB=your_policies_database_id
NOTION_ACTION_LEDGER_DB=your_action_ledger_database_id
NOTION_AUDIT_EVIDENCE_DB=your_audit_evidence_database_id

# Marketing Platform APIs
MAILCHIMP_API_KEY=your_mailchimp_api_key
MAILCHIMP_DEFAULT_LIST=your_default_list_id
REPLY_EMAIL=security@yourdomain.com

LINKEDIN_TOKEN=your_linkedin_api_token
LINKEDIN_COMPANY_ID=your_company_id

META_ACCESS_TOKEN=your_meta_access_token
META_AD_ACCOUNT_ID=your_ad_account_id
META_CAMPAIGN_ID=your_campaign_id

# Vimeo Integration
VIMEO_API_TOKEN=your_vimeo_api_token
VIMEO_MARKETING_SHOWCASE=your_marketing_showcase_id
VIMEO_CLIENT_SHOWCASE=your_client_showcase_id
VIMEO_INSPIRATION_SHOWCASE=your_inspiration_showcase_id
VIMEO_MICROCONTENT_SHOWCASE=your_microcontent_showcase_id

# WordPress Integration
WORDPRESS_SITES=https://site1.com,https://site2.com
WORDPRESS_API_KEY=your_wordpress_api_key

# Production Settings
NODE_ENV=production
PORT=8080
POLL_INTERVAL=5
RUN_ONCE=false
LOG_LEVEL=info

# Security
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100
SESSION_SECRET=your-session-secret-here

# Performance Monitoring
HEALTH_CHECK_INTERVAL=30
METRICS_COLLECTION_ENABLED=true
ALERT_WEBHOOK_URL=your_alert_webhook_url